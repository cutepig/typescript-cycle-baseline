{
  "name": "money-funny-sonny",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "browserify": "browserify src/index.tsx -p tsify -o public/index.js",
    "watchify": "watchify src/index.tsx -v -p tsify -o public/index.js",
    "budo": "budo src/index.tsx:index.js --dir assets --live -- -p tsify",
    "assets": "bash -c \"mkdir -p public && cp -r assets/* public\"",
    "server": "http-server public",
    "clean": "bash -c \"[ -d public ] && rm -r public\"",
    "build": "npm run clean && npm run assets && npm run browserify",
    "start": "npm run budo"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@cycle/dom": "^10.0.0-rc9",
    "@cycle/rxjs-run": "^3.0.2",
    "browserify": "^13.0.1",
    "rxjs": "^5.0.0-beta.9",
    "tsify": "^0.16.0",
    "typescript": "^1.8.10",
    "xstream": "^5.0.6"
  },
  "devDependencies": {
    "budo": "^8.3.0",
    "tslint": "^3.12.1",
    "tslint-immutable": "^0.1.2",
    "typings": "^1.3.0"
  }
}
